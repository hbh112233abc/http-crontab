{
    "name": "bingher/think-crontab",
    "description": "ThinkPHP接口化定时任务管理",
    "type": "library",
    "license": [
        "Apache-2.0"
    ],
    "keywords": [
        "crontab",
        "http"
    ],
    "authors": [
        {
            "name": "cshaptx4869",
            "email": "994774638@qq.com"
        },
        {
            "name": "hbh112233abc",
            "email": "hbh112233abc@163.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "ext-json": "*",
        "ext-pdo_sqlite": "*",
        "workerman/workerman": "^4.0",
        "workerman/crontab": "^1.0",
        "topthink/framework": "^8.1",
        "topthink/think-orm": "^3.0|^4.0",
        "nikic/fast-route": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "bingher\\crontab\\": "src"
        }
    },
    "extra": {
        "think": {
            "services": [
                "bingher\\crontab\\Service"
            ],
            "config": {
                "crontab": "src/config/crontab.php"
            }
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        },
        {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    ],
    "require-dev": {
        "bingher/think-test": "^1.0",
        "topthink/think-cache": "^3.0"
    }
}
